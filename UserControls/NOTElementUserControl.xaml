<UserControl x:Class="SPZCapstoneVar2.UserControls.NOTElementUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SPZCapstoneVar2.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="9*"/>
            <ColumnDefinition Width="151*"/>
        </Grid.ColumnDefinitions>
        <Rectangle HorizontalAlignment="Left" Height="100" Stroke="Black" VerticalAlignment="Top" Width="80" Margin="-41,-48,0,0" Fill="White"/>
        <Label Content="1" HorizontalAlignment="Left" Margin="-16,-27,0,0" VerticalAlignment="Top" FontSize="36"/>
        <local:ConnectionPinUserControl x:Name="OutputPin" Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="100" Margin="36,-1,0,0" VerticalAlignment="Top" Width="100"/>
        <local:ConnectionPinUserControl x:Name="InputPin" Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="100" Margin="-43,0,0,0" VerticalAlignment="Top" Width="100"/>

    </Grid>
</UserControl>
